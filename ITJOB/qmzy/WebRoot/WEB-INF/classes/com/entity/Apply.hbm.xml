<?xml version="1.0" encoding="utf-8"?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.entity.Apply" table="apply">
		<id name="sid" column="sid" type="int">
			<generator class="increment" />
		</id>

	
		<property name="name" column="name" type="string">
		</property>
		<property name="job" column="job" type="string">
		</property>
		<property name="education" column="education" type="string">
		</property>
		<property name="exp" column="exp" type="string">
		</property>
		<property name="information" column="information" type="string">
		</property>
		<property name="date" column="date" type="date">
		</property>

	</class>
</hibernate-mapping>
  
